%import common.WS
%import common.ESCAPED_STRING
%import common.SIGNED_NUMBER
%import common.CNAME
%ignore WS

start:      filter*
filter:     CNAME (operator rhs)?

operator:   "="
            | "!="
            | "<"
            | ">"
            | "<="
            | ">="

rhs:        ESCAPED_STRING
            | SIGNED_NUMBER
            | (CNAME | "%")+        // % for sql 'like'